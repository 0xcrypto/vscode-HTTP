{
  "name": "http",
  "displayName": "HTTP",
  "description": "Syntax highlighting for HTTP requests and responses",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "http",
      "aliases": ["HTTP", "http"],
      "extensions": [".http"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "http",
      "scopeName": "source.http",
      "path": "./syntaxes/http.tmLanguage.json"
    }]
  }
}
